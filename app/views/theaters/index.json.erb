<%= 
  {
    :theaters => @theaters.map{|theater|
      theater.attributes.merge(
        :shows => theater.shows.map{|show|
          {
            :movie_id => show.movie_id,
            :times    => show.data
          }
        }
      )
    },
    :movies => @movies.map{|movie|
      movie.attributes
    }
  }.to_json
%>